{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "module": "esnext",
    "baseUrl": "./src",
    "paths": {
      "@data/*": ["data/*"],
      "@contactoperations/*": ["contactoperations/*"],
      "@presentation/*": ["presentation/*"],
      "@utils/*":["utils/*"],
      "@hooks/*":["hooks/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}